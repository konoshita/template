<section class="content">
      <div class="container-fluid">
        <div class="row">
          <%= render 'dashboard/user'%>
                <div class="col-md-9">
                <div class="card card-primary card-outline">
                  <div class="card-header">
                    <div class="container mb-5 pt-2">
                      <div class="row">
                        <div class="col-12">
                          <div class="box box-primary">
                            <div class="box-body table-responsive no-padding" style="height: 90vh;">
                              <table class="table table-hover table-head-fixed text-nowrap">
                                <thead>
                                  <tr>
                                    <th scope="col">ID</th>
                                    <th scope="col">タイトル</th>
                                    <th scope="col">詳細</th>
                                    <th scope="col">編集</th>
                                    <th scope="col"></th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <% if @oders.present? %>
                                    <%= render @oders %>
                                  <% else %>
                                    <p><%= t('.no_result') %></p>
                                  <% end %>
                                </tbody>
                              </table>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- /.row -->
              </div>
                </div>
                <!-- /.card -->
              </div>
</section>